{
  "name": "labs-website",
  "version": "1.0.0",
  "private": true,
  "homepage": "http://excellalabs.s3-website-us-east-1.amazonaws.com",
  "devDependencies": {
    "concurrently": "^3.4.0",
    "eslint": "^3.16.1",
    "eslint-plugin-react": "^6.10.0",
    "json-server": "^0.9.5",
    "node-sass-chokidar": "0.0.2",
    "npm-run-all": "^4.0.2",
    "react-scripts": "0.9.0"
  },
  "dependencies": {
    "moment": "^2.18.1",
    "react": "^15.4.2",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0"
  },
  "scripts": {
    "prestart": "npm run build",
    "start-js": "concurrently \"react-scripts start\" \"npm run rest\" ",
    "start": "npm-run-all -p watch-css start-js",
    "rest": "json-server --watch --port 4000 db.json",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "aws:login": "aws ecr get-login --no-include-email --region us-east-1",
    "image:build": "npm run build && docker build -t excella-labs .",
    "image:run": "docker run -p 8080:3000 excella-labs",
    "image:tag": "docker tag excella-labs:latest 090999229429.dkr.ecr.us-east-1.amazonaws.com/excella-labs:latest",
    "image:publish": "docker push 090999229429.dkr.ecr.us-east-1.amazonaws.com/excella-labs:latest",
    "aws:release": "aws ecs run-task --cluster efds-cluster --task-definition efds-task-definition-front-end:1 --placement-constraints type=\"memberOf\",expression=\"attribute:EFDS_Server == Front_End\"",
    "aws:stop_instance": "aws ecs stop-task --cluster efds-cluster --task ac1a33af-134d-4387-9356-081d91a3e549",
    "aws:describe": "aws ecs describe-tasks --cluster efds-cluster --task ac1a33af-134d-4387-9356-081d91a3e549",
    "aws:list": "aws ecs list-tasks --cluster efds-cluster"
  },
  "authors": [
    "Doguhan Uluca<doguhan.uluca@excella.com>",
    "Sean Killeen<sean.killeen@excella.com>",
    "Stephen Brody<stephen.brody@excella.com>"
  ],
  "description": "Future home of ExcellaLabs.com   #### [Production URL on Amazon S3](http://excellalabs.s3-website-us-east-1.amazonaws.com) #### [Development URL on Amazon S3](http://excellalabs.dev.s3-website-us-east-1.amazonaws.com/)",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/excellalabs/labs-website.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/excellalabs/labs-website/issues"
  }
}
